page {
  background: #F4F5F6;
}
.sendMessage {
  font-size: 30rpx;
  width: 18%;
  text-align: left;
  height: 80rpx;
}
.Reg .content-wrapper {
  width: 100%;
}
.Reg .content-wrapper .title {
  margin-top: 10rpx;
  width: 100%;
  margin-bottom: 10px;
}
.Reg .content-wrapper .title ._h1 {
  border: 0px;
  width: 50%;
  margin: 0 auto;
  text-align: center;
  border-bottom: 1px solid #E3E3E3;
  height: 50px;
  line-height: 50px;
  font-size: 17px;
  overflow: hidden;
  font-weight: 400;
}
.Reg .content-wrapper .login-form {
  margin: 40rpx 20rpx 40rpx 30rpx;
  background: #FFFFFF;
}
.Reg .content-wrapper .login-form .login-form-items {
  padding: 25rpx 18rpx;
  border-bottom: 2rpx solid #F3F4F5;
  position: relative;
  display: flex;
}
.Reg .content-wrapper .login-form .login-form-items .login-form-items-title {
  width: 35%;
  line-height: 50rpx;
  height: 55rpx;
  flex-shrink: 0;
}
.Reg .content-wrapper .login-form .login-form-items .login-input {
  width: 100%;
}
.Reg .content-wrapper .login-form .login-form-items .login-input1 {
  width: 80%;
}
.Reg .login-API {
  height: 150rpx;
  margin-top: 15rpx;
  display: flex;
  justify-content: center;
}
.Reg .login-API-txt {
  text-align: center;
  margin-top: 0;
}
.Reg .login-API-weixin {
  height: 90rpx;
  width: 80rpx;
}
.Reg .login-API-weixin-image {
  width: 100%;
  height: 75%;
}
.Reg .login-API-weixin-txt {
  width: 100%;
  height: 25%;
}
.Reg .submit-wrapper {
  padding: 10px;
  padding-top: 10px;
}
.Reg .login-btn2 {
  background-color: "green";
}

