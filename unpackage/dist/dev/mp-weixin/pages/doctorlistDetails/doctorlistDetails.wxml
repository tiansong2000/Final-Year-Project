<view><view class="top"><view class="top-left"><view class="name">{{doctorDetail.doctorname}}</view><view class="title">{{doctorDetail.title}}</view><view class="department">{{doctorDetail.department}}</view><view class="hospital">独墅湖医院</view></view><view class="top-right"><image class="pic" src="{{doctorDetail.image}}" mode="aspectFill"></image></view></view><view class="middle"><view class="icon_text"><image class="icon1" src="../../static/disease.png"></image><text class="text1">擅长疾病</text><image class="{{['icon',(showMore1)?'displayNone':'']}}" src="../../static/doctorlistDetail/close.png" data-event-opts="{{[['tap',[['showSpecialist',['$event']]]]]}}" bindtap="__e"></image><image class="{{['icon',(!showMore1)?'displayNone':'']}}" src="../../static/doctorlistDetail/open.png" data-event-opts="{{[['tap',[['showSpecialist',['$event']]]]]}}" bindtap="__e"></image></view><view class="specific-area">{{doctorDetail.specialist}}</view></view><view class="bottom"><view class="icon_text"><image class="icon1" src="../../static/personaldetail.png"></image><text class="text1">个人简历</text><image class="{{['icon',(showMore2)?'displayNone':'']}}" src="../../static/doctorlistDetail/close.png" data-event-opts="{{[['tap',[['showPersonal',['$event']]]]]}}" bindtap="__e"></image><image class="{{['icon',(!showMore2)?'displayNone':'']}}" src="../../static/doctorlistDetail/open.png" data-event-opts="{{[['tap',[['showPersonal',['$event']]]]]}}" bindtap="__e"></image></view><view class="personal-introduction">{{doctorDetail.detail}}</view></view><view>预约时间表</view><view class="timetable"><view class="current-timeslot"><view class="uni-input"><text data-event-opts="{{[['tap',[['toReservationTable',[0]]]]]}}" class="uni-inputdate" bindtap="__e">{{todaydate}}</text><text class="uni-inputweek">{{todayWeek}}</text></view><view class="uni-input"><text data-event-opts="{{[['tap',[['toReservationTable',[1]]]]]}}" class="uni-inputdate" bindtap="__e">{{dateplus1}}</text><text class="uni-inputweek">{{weekplus1}}</text></view><view class="uni-input"><text class="uni-inputdate">{{dateplus2}}</text><text class="uni-inputweek">{{weekplus2}}</text></view><view class="uni-input"><text class="uni-inputdate">{{dateplus3}}</text><text class="uni-inputweek">{{weekplus3}}</text></view><view class="uni-input"><text class="uni-inputdate">{{dateplus4}}</text><text class="uni-inputweek">{{weekplus4}}</text></view><view class="uni-input"><text class="uni-inputdate">{{dateplus5}}</text><text class="uni-inputweek">{{weekplus5}}</text></view></view></view></view>